{
    "name": "Jupyter with Dev Container Compatibility",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "remoteUser": "root",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "forwardPorts": [8888],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    }
}